<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
    <context:annotation-config/> <!-- makes annotation available/ enables annotations-->
	
	<bean class="com.kaustubh.autowire.annotation.Address" name="temp">
		<property name="city" value="Delhi"> </property>
		<property name="street" value="Temp Street at Delhi"></property>
	</bean>

	<bean class="com.kaustubh.autowire.annotation.Address" name="temp2">
		<property name="city" value="Pune"/>
		<property name="street" value="Temp Street in Pune"/>
	</bean>
	
	<bean class="com.kaustubh.autowire.annotation.Employee" name="employee">
		
		
	</bean>

</beans>