<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	
   	<bean class="com.kaustubh.autowire.usingXML.Address" name="address1">
		<property name="city" value="Pune"> </property>
		<property name="street" value="Temp Street"></property>
	</bean>
	
	<bean class="com.kaustubh.autowire.usingXML.Address" name="address">
		<property name="city" value="Delhi"> </property>
		<property name="street" value="Temp Street at Delhi"></property>
	</bean>
	
	<bean class="com.kaustubh.autowire.usingXML.Employee" name="employee" autowire="byName">
		
		
	</bean>

</beans>